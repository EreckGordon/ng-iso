{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      { 
        "source":"/worker-basic.min.js", 
        "headers": [
          {"key": "Cache-Control", 
          "value": "no-cache"
          }
        ] 
      }      
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
